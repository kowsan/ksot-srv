<p id="notice"><%= notice %></p>

<h1>Listing Control List Factor Groups</h1>

<table>
  <thead>
    <tr>
      <th>Order num</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @control_list_factor_groups.each do |control_list_factor_group| %>
      <tr>
        <td><%= control_list_factor_group.order_num %></td>
        <td><%= control_list_factor_group.name %></td>
        <td><%= link_to 'Show', control_list_factor_group %></td>
        <td><%= link_to 'Edit', edit_control_list_factor_group_path(control_list_factor_group) %></td>
        <td><%= link_to 'Destroy', control_list_factor_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Control list factor group', new_control_list_factor_group_path %>
