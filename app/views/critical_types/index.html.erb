

<h4 style="color: orange">Список типов критичности </h4>

<table class="table table-hover table-striped">

      <th>Наименование</th>
      <th colspan="3"></th>



  <tbody>
    <% @critical_types.each do |critical_type| %>
      <tr>
        <td><%= critical_type.name %></td>

        <td><%= link_to 'Редактировать', edit_critical_type_path(critical_type) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Critical type', new_critical_type_path %>
