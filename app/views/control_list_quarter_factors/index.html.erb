<p id="notice"><%= notice %></p>

<h1>Listing Control List Quarter Factors</h1>

<table>
  <thead>
    <tr>
      <th>Order num</th>
      <th>Factor</th>
      <th>Is active</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @control_list_quarter_factors.each do |control_list_quarter_factor| %>
      <tr>
        <td><%= control_list_quarter_factor.order_num %></td>
        <td><%= control_list_quarter_factor.factor %></td>
        <td><%= control_list_quarter_factor.is_active %></td>
        <td><%= control_list_quarter_factor.comment %></td>
        <td><%= link_to 'Show', control_list_quarter_factor %></td>
        <td><%= link_to 'Edit', edit_control_list_quarter_factor_path(control_list_quarter_factor) %></td>
        <td><%= link_to 'Destroy', control_list_quarter_factor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

